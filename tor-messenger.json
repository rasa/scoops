{
    "autoupdate": {
        "hash": {
            "url": "$baseurl/sha256sums-signed-build.txt"
        },
        "url": "https://dist.torproject.org/tormessenger/$version/tormessenger-install-$version_en-US.exe"
    },
    "bin": [
        [
            "Browser/instantbird.exe",
            "instantbird"
        ],
        [
            "Browser/instantbird.exe",
            "tormessenger"
        ]
    ],
    "checkver": {
        "re": "current version of Tor Messenger is.*v([\\d.abc]+)",
        "url": "https://trac.torproject.org/projects/tor/wiki/doc/TorMessenger#Downloads"
    },
    "description": "Multi-protocol secure chat client",
    "hash": "5d08c010f4d5b114319f3808e08c1b0ceef7858e1ea94b09ae62d372995a89aa",
    "homepage": "https://trac.torproject.org/projects/tor/wiki/doc/TorMessenger",
    "installer": {
        "args": [
            "/S",
            "/D=$dir"
        ]
    },
    "license": "GPL-2.0",
    "shortcuts": [
        [
            "Browser/instantbird.exe",
            "Tor Messenger"
        ]
    ],
    "url": "https://dist.torproject.org/tormessenger/0.5.0b1/tormessenger-install-0.5.0b1_en-US.exe",
    "version": "0.5.0b1"
}
