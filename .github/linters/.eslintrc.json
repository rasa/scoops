{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/eslintrc.json",
  "overrides": [
    {
      "extends": [
        "plugin:jsonc/recommended-with-jsonc"
      ],
      "files": [
        "*.json"
      ],
      "parser": "jsonc-eslint-parser",
      "parserOptions": {
        "jsonSyntax": "JSONC"
      }
    },
    {
      "extends": [
        "plugin:jsonc/recommended-with-jsonc"
      ],
      "files": [
        "*.jsonc"
      ],
      "parser": "jsonc-eslint-parser",
      "parserOptions": {
        "jsonSyntax": "JSONC"
      }
    },
    {
      "extends": [
        "plugin:jsonc/recommended-with-json5"
      ],
      "files": [
        "*.json5"
      ],
      "parser": "jsonc-eslint-parser",
      "parserOptions": {
        "jsonSyntax": "JSON5"
      }
    }
  ]
}